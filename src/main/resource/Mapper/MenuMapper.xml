<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sjw.ShiroTest.Dao.MainDao">
    <resultMap id="mainMenu" type="map">
        <result column="ID" javaType="int" jdbcType="INTEGER"></result>
        <result column="NAME" javaType="string" jdbcType="VARCHAR"></result>
    </resultMap>

    <select id="getMainMenu" resultMap="mainMenu">
        SELECT ID,NAME FROM MENU
    </select>
</mapper>